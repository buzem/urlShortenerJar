# The servers to connect to. Comma separated.
servers=http://localhost:7001/

# The number of records loaded in the initial database.
recordcount=1000

# The number of read and write operations to execute.
operationcount=100000

# The proportions of read and write operations.
readproportion=0.9
insertproportion=0.1

# The distribution of read and write operations over the keyspace.
requestdistribution=latest


